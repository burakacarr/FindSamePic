                android:layout_width="100dp"
                android:layout_height="150dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/soru" />

            <ImageView
                android:onClick="ResimDegistir"
                android:id="@+id/imageView11"
                android:layout_width="100dp"
                android:layout_height="150dp"
